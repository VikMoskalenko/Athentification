@model IEnumerable<string>
@{
    ViewData["Title"] = "ShowImagesFromFolder";
}
<h1>Show Images From Folder</h1>
<div class="row">
    <div class="col-md-4">
        @foreach (var fileName in Model)
        {
            <img src="@Url.Content("~/NewFolder/" + fileName)" width="200" height="200" alt="Image Not Availanle" class="img-fluid" />
        }
    </div>